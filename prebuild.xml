      <Reference name="gdk-sharp" version="2.8.0.0" />
      <Reference name="glade-sharp" version="2.8.0.0" />
      <Reference name="gtk-sharp" version="2.8.0.0" />
      <Reference name="glib-sharp" version="2.8.0.0" />
