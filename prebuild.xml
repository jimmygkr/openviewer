      <Reference name="atk-sharp" />
      <Reference name="gdk-sharp" />
      <Reference name="glade-sharp" />
      <Reference name="glib-sharp" />
      <Reference name="gtk-sharp" />
      <?endif ?>
      <Reference name="OpenViewer.Framework"/>
      <Reference name="OpenViewer.UI"/>
      <Reference name="OpenViewer.Models"/>
      <Reference name="OpenViewer.Render"/>
      <Files>
